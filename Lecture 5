INSERT INTO ITEMS_RECEIVED (PO_CODE, ITEM_CODE, PO_QTY, PO_AMOUNT, WAREHOUSE_CODE)
VALUES('PO2012','MC5003','5','12500','WHMAK'),
('PO2014','MC5006','10','5000','WHMAK'),
