
CREATE DATABASE Lec5Exer1

Use Lec5Exer1

CREATE TABLE ITEM
(ITEM_CODE VARCHAR (20) PRIMARY KEY,
ITEM_NAME VARCHAR (20),
ITEM_PRICE INT,
SUPPLIER_NAME VARCHAR (20),
SUPPLIER_CODE VARCHAR (30),
WAREHOUSE_CODE VARCHAR (20),
WAREHOUSE_LOCATION VARCHAR (20));

CREATE TABLE PURCHASE_ORDERS
(PO_CODE VARCHAR (20) PRIMARY KEY,
ITEM_CODE VARCHAR (20),
PO_QTY INT,
PO_AMOUNT INT,
WAREHOUSE_CODE VARCHAR (20));

CREATE TABLE ITEMS_RECEIVED
(PO_CODE VARCHAR (20),
ITEM_CODE VARCHAR (20),
PO_QTY INT,
PO_AMOUNT INT,
WAREHOUSE_CODE VARCHAR (20));

INSERT INTO ITEM (ITEM_CODE, ITEM_NAME, ITEM_PRICE, SUPPLIER_CODE, SUPPLIER_NAME, WAREHOUSE_CODE, WAREHOUSE_LOCATION)
