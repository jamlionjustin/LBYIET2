('PO2016','MC5001','1','35000','WHPAS'),
('PO2017','MC5002','1','25000','WHMAN'),
('PO2022','MC5001','1','35000','WHPAS'),
('PO2023','MC5002','1','25000','WHMAN');

SELECT * FROM ITEMS_RECEIVED;

--Number 1
SELECT * FROM ITEMS_RECEIVED
UNION ALL
SELECT * FROM PURCHASE_ORDERS;

--Number 2
SELECT * FROM ITEMS_RECEIVED
UNION
SELECT * FROM PURCHASE_ORDERS

--Number 3
SELECT * FROM ITEMS_RECEIVED LEFT JOIN PURCHASE_ORDERS 
ON PURCHASE_ORDERS.PO_CODE = ITEMS_RECEIVED.PO_CODE WHERE PURCHASE_ORDERS.PO_CODE IS NULL

--Number 4
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY <= ALL (SELECT PO_QTY FROM PURCHASE_ORDERS);

--Number 5
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY >= ALL (SELECT PO_QTY FROM PURCHASE_ORDERS);

--Number 6
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY <= ANY (SELECT PO_QTY FROM PURCHASE_ORDERS);

--Number 7
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY >= ANY (SELECT PO_QTY FROM PURCHASE_ORDERS);

--Number 8
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY <= ANY (SELECT PO_QTY FROM PURCHASE_ORDERS WHERE PO_QTY = 5);

--Number 9
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY >= ANY (SELECT PO_QTY FROM PURCHASE_ORDERS WHERE PO_QTY = 5);

--Number 10
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY <= ALL (SELECT PO_QTY FROM PURCHASE_ORDERS WHERE PO_QTY = 5);

--Number 11
SELECT * FROM PURCHASE_ORDERS
WHERE PO_QTY >= ALL (SELECT PO_QTY FROM PURCHASE_ORDERS WHERE PO_QTY = 5);

--Number 12
SELECT WAREHOUSE_CODE, WAREHOUSE_LOCATION INTO WAREHOUSE FROM ITEM
SELECT * FROM WAREHOUSE

--Number 13
SELECT SUPPLIER_CODE, SUPPLIER_NAME INTO SUPPLIER FROM ITEM
SELECT * FROM SUPPLIER

--Number 14
ALTER TABLE WAREHOUSE
ADD WAREHOUSE_MANAGER VARCHAR(20);
SELECT * FROM WAREHOUSE

--Number 15
UPDATE WAREHOUSE SET WAREHOUSE_MANAGER ='Manny' WHERE WAREHOUSE_LOCATION = 'Makati'
UPDATE WAREHOUSE SET WAREHOUSE_MANAGER ='Jessica' WHERE WAREHOUSE_LOCATION = 'Mandaluyong'
UPDATE WAREHOUSE SET WAREHOUSE_MANAGER ='Flloyd' WHERE WAREHOUSE_LOCATION = 'Pasig'
SELECT * FROM WAREHOUSE
