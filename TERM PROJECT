CREATE DATABASE DELEONKINGMULTIVITE

USE DELEONKINGMULTIVITE

CREATE TABLE UniqueIDs
(UNIT CODE VARCHAR (20),
UNIT VARCHAR (20),
ITEM CATEGORY CODE VARCHAR (20),
ITEM CATEGORY VARCHAR (40),
ITEM SUPPLIER CODE VARCHAR (20),
ITEM SUPPLIER NAME (VARCHAR (40),
BUSINESS_PARTNER SUPPLY CATEGORY CODE VARCHAR (20),
SUPPLY CATEGORY VARCHAR (40),
SEX CODE VARCHAR (20),
SEX VARCHAR (20),
SALES POSITION CODE VARCHAR (20),
SALES POSITION VARCHAR (40));

CREATE TABLE ItemMaster
(ITEM CODE VARCHAR (20),
ITEM NAME VARCHAR (40),
QUANTITY INT,
UNIT VARCHAR (20),
CATEGORY VARCHAR (40),
SUPPLIER VARCHAR (40));

CREATE TABLE BusinessPartners
(BUSINESS_PARTNER CODE VARCHAR (20),
COMPANY NAME VARCHAR (40),
SUPPLY CATEGORY VARCHAR (40),
RM DISCOUNT INT,
EQUIPMENT DISCOUNT INT);

CREATE TABLE Personnel
(DOCTOR CODE VARCHAR (20),
SURNAME VARCHAR (40),
FIRSTNAME VARCHAR (40),
AGE INT,
SEX VARCHAR (20),
SPECIALTY_POSITION VARCHAR (40));

CREATE TABLE OrderList
(ORDER_CODE VARCHAR (20),
ORDER_CATEGORY VARCHAR (40),
PRICE INT);

CREATE TABLE CustomerRecord
(CUSTOMER CODE VARCHAR (20),
CUSTOMER NAME VARCHAR (40),
LOCATION VARCHAR (20),
BUSINESS_CATEGORY VARCHAR (20));

